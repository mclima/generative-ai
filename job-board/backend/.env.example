# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/jobboard

# Redis Cache Configuration (optional - app works without Redis)
REDIS_URL=redis://localhost:6379/0

# LLM Provider Configuration
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o-mini
LLM_TEMPERATURE=0.3

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration (optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Job API Keys
USAJOBS_EMAIL=your_email@example.com
USAJOBS_API_KEY=your_usajobs_api_key

# RapidAPI Key (used by JSearch and LinkedIn agents)
# Sign up at: https://rapidapi.com/
RAPIDAPI_KEY=your_rapidapi_key_here
